import java.util.Scanner;

public class Ciclo {

    public static void main(String[] args){

        Scanner entrada = new Scanner(System.in); 
        int numero;
        long factorial=1; 
        numero = entrada.nextInt(); 
        for(int i=1;i<=numero;i++){
            factorial = factorial * i;
        } 
        System.out.print(factorial); 
    }    
    