class E3{
    public static double mean(int [] vector){ 
        float suma = 0;
        float promedio = 0; 
        for(int i=0; i < vector.length; i++){ 
            suma = vector[i] + suma; 
            promedio = suma / vector.length; 
           
             
        }
        
        
        
        
        return promedio;
    }
}